# User Bot Configuration Example
# Скопируйте этот файл в .env и заполните своими данными

# User Bot Telegram Token
TELEGRAM_TOKEN=your_user_bot_token_here

# OpenAI API Key
OPENAI_API_KEY=your_openai_api_key_here
ASSISTANT_ID=your_assistant_id_here

# PostgreSQL Database (SHARED with Admin Bot)
DB_HOST=localhost
DB_PORT=5432
DB_NAME=telegram_bot
DB_USER=bot_user
DB_PASSWORD=your_database_password_here

# Super Admin Telegram ID
SUPER_ADMIN_ID=your_telegram_id_here

# Admin Bot Token (for notifications)
ADMIN_BOT_TOKEN=your_admin_bot_token_here
ADMIN_CHANNEL_ID=-1001234567890
ADMIN_CHANNEL_THREAD_ID=2

# Settings
LOG_LEVEL=INFO
MAX_WORKERS=5
